<h1>Rapper追加</h1>
<div>
<%= form_for(@rapper) do |f|%>
  <div><%= f.label :名前 %></div>
  <div><%= f.text_field :name %></div>
  <div><%= f.label :説明 %></div>
  <div><%= f.text_area :description %></div>
  <div><%= f.label :画像 %></div>
  <div><%= f.file_field :image %></div>
  <div><%= f.submit %></div>
<% end %>
</div>
